{
  "name": "speed-reader-extension",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "rollup -c && cp src/content.css public/content.css",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "clean": "rm -rf public/build public/content.js public/content.js.map public/content.css",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "check": "svelte-check",
    "test2": "jest",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-interactions": "^8.3.5",
    "@storybook/addon-links": "^8.3.5",
    "@storybook/addon-svelte-csf": "^4.1.7",
    "@storybook/blocks": "^8.3.5",
    "@storybook/svelte": "^8.3.5",
    "@storybook/svelte-vite": "^8.3.5",
    "@storybook/test": "^8.3.5",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/svelte": "^5.2.4",
    "@tsconfig/svelte": "^3.0.0",
    "@types/chrome": "^0.0.278",
    "@types/jest": "^29.5.13",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^2.75.6",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "storybook": "^8.3.5",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "svelte-jester": "^5.0.0",
    "svelte-preprocess": "^5.1.4",
    "ts-jest": "^29.2.5",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "sirv-cli": "^2.0.0"
  }
